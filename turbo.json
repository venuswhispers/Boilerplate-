{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "//#lint": {},
    "lint:fix": {},
    "//#lint:fix": {}
  },
  "globalDependencies": [".env", ".env.local", "tsconfig.json", ".eslintrc.json", ".prettierrc.js"],
  "globalEnv": ["NEXT_PUBLIC_PRODUCTION_MODE", "NEXT_PUBLIC_URL", "NEXT_PUBLIC_RPC_ENDPOINT"]
}
