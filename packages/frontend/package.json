{
  "name": "@inkathon/frontend",
  "version": "0.0.1",
  "homepage": "https://inkathon.xyz",
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "node": "pnpm run --filter contracts node",
    "dev-and-node": "concurrently \"pnpm dev\" \"pnpm node\" --names \"Next,Node\" --kill-others",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "prettier . --check && pnpm eslint",
    "lint:fix": "prettier . --write && pnpm eslint --fix",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@azns/resolver-core": "^0.0.5",
    "@azns/resolver-react": "^0.0.5",
    "@chakra-ui/react": "^2.7.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@inkathon/contracts": "workspace:*",
    "@polkadot/api": "^10.9.1",
    "@polkadot/api-contract": "^10.9.1",
    "@polkadot/extension-dapp": "^0.46.5",
    "@polkadot/extension-inject": "^0.46.5",
    "@polkadot/types": "^10.9.1",
    "@polkadot/util": "^12.3.2",
    "@polkadot/util-crypto": "^12.3.2",
    "@scio-labs/use-inkathon": "^0.0.1-alpha.51",
    "next": "^13.4.9",
    "next-seo": "^6.1.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "sharp": "^0.32.1",
    "spinners-react": "^1.0.7",
    "use-async-effect": "^2.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@testing-library/react": "^14.0.0",
    "@types/downloadjs": "^1.4.3",
    "@types/node": "^18.16.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-macros": "^3.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^3.0.0",
    "prettier-plugin-twin.macro": "^1.0.14",
    "tailwindcss": "^3.3.2",
    "twin.macro": "^3.3.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1"
  }
}
